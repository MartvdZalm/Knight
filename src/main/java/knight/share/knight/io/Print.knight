fn length(): void
{
	asm {
		"movq %rdi, %rsi"
		"xorq %rcx, %rcx"
		"count_loop:"
		"cmpb $0, (%rsi, %rcx)"
		"je end_loop"
		"incq %rcx"
		"jmp count_loop"
		"end_loop:"
	}
}

fn print(string input): void
{
	asm {
		"call length"
		"movq $1, %rax"
		"movq %rdi, %rsi"
		"xor %rdi, %rdi"
		"movq %rcx, %rdx"
		"syscall"
	}
}